<Window x:Class="ChqPrint.VentanaConfiguracion"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Configuración" Height="200" Width="400" Loaded="Window_Loaded" Unloaded="Window_Unloaded" Background="DarkSeaGreen" Icon="/ChqPrint;component/Media/Configuration-Settings-icon.png">
    <Grid>
        <CheckBox Content="Permitir escritura manual en el campo 'Páguese a la Orden de'" Height="16" HorizontalAlignment="Left" Margin="12,12,0,0" Name="checkBoxPermitirEscrituraManual" VerticalAlignment="Top" />
        <Button Content="Cancelar" Height="26" HorizontalAlignment="Right" Margin="0,0,12,12" Name="buttonCancelar" VerticalAlignment="Bottom" Width="120" Click="buttonCancelar_Click" />
        <Button Content="Aceptar" Height="26" HorizontalAlignment="Right" Margin="0,0,138,12" Name="buttonAceptar" VerticalAlignment="Bottom" Width="120" Click="buttonAceptar_Click" />
    </Grid>
</Window>
